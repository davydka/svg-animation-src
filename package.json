{
  "name": "svg-anims",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@11ty/eleventy": "^0.9.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "case": "^1.6.1",
    "classnames": "^2.2.6",
    "clean-css": "^4.2.1",
    "css-property-parser": "^1.0.6",
    "css-selector-parser": "^1.3.0",
    "dedent": "^0.7.0",
    "eases": "^1.0.8",
    "gl-matrix": "^3.0.0",
    "glob": "^7.1.5",
    "html-minifier": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.11",
    "markdown-it": "^10.0.0",
    "markdown-it-sup": "^1.0.0",
    "mkdirp": "^0.5.1",
    "node-watch": "^0.6.0",
    "normalize.css": "^8.0.1",
    "perfectionist": "^2.4.0",
    "prettier": "^1.16.4",
    "seedrandom": "^3.0.1"
  },
  "scripts": {
    "build": "echo build",
    "clean": "rm -rfv dist",
    "build-site": "yarn run clean; eleventy",
    "watch-site": "DEBUG=Eleventy* eleventy --watch --serve",
    "build-svgs": "yarn run clean; babel-node scripts/svgs/build.js --presets env",
    "watch-svgs": "babel-node scripts/svgs/watch.js --presets env"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
